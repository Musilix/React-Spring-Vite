generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      =  env("DB_COMPOUND_URL")
}

model Users {
  id    Int     @id @default(autoincrement())
  username String  @unique @db.VarChar(255)
  currentGoal Int @default(0)
  count  Int
  createdAt DateTime? @default(now())
  updatedAt DateTime? @updatedAt
}